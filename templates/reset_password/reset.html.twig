                            {{ form_errors(resetForm.plainPassword.first) }}
                        </div>

                        <div class="mb-3">
                            {{ form_label(resetForm.plainPassword.second, 'Confirmez le mot de passe', {'label_attr': {'class': 'form-label fw-bold'}}) }}
                            {{ form_widget(resetForm.plainPassword.second, {'attr': {'class': 'form-control', 'placeholder': 'Répétez votre mot de passe'}}) }}
