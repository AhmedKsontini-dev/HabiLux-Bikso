                                {% endfor %}

                        {{ form_start(resetForm) }}
                        
                        <div class="mb-3">
                            {{ form_label(resetForm.plainPassword.first, 'Nouveau mot de passe', {'label_attr': {'class': 'form-label fw-bold'}}) }}
